class Main {
    public static void main(String[] args) {
        int arr[] = {2,6,5,8,11};
        int n =arr.length;
        int target = 14;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(i==j) {continue;}
                if(arr[i]+arr[j]==target){
                    System.out.println(i+" "+j);
                    break;
                }
            }   
        }
    }
}
