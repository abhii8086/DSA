package basicMaths; ///////!***!****

public class ArmstrongNo {

	public static void main(String[] args) {

		int n = 371;
		int dup = n; // save the copy for compair
		int sum = 0;
		while (n > 0) {
			int ld = n % 10;
			n = n / 10;
			sum = sum + ld * ld * ld;
		}
		if (sum == dup)
			System.out.println("true");
		else
			System.out.println("false");
	}

}
