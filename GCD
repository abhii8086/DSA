class Solution {

//		brute force approach says that

		int n1 = 7;
		int n2 = 6;
		int gcd = 1; // for every no gcd is one if nothing like for prime nos

		for (int i = 1; i <= Math.min(n1, n2); i++) {
			if (n1 % i == 0 && n2 % i == 0)
				gcd = i;
		}
		System.out.println(gcd);
//optimize logic
    public int GCD(int n1, int n2) {
        while(n1>0 && n2>0){
            if(n1>n2) n1 %=n2;
            else n2 %=n1;
        }
        if(n1==0) return n2;
            else return n1;
    }
}
