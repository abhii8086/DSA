public class patterns {

	public static void main(String[] args) {
//		 A
//		 B B
//		 C C C
//		 D D D D
//		 E E E E E

		int n = 5;
		for (int i = 1; i <= n; i++) {
			char ch = (char) ('A' + i - 1); // need to type cast
			for (int j = 1; j <= i; j++) {
				System.out.print(" " + ch);
			}
			System.out.println();
		}

	}

}
