class Main {
    public static void main(String[] args) {
        int arr[] = {2,4,5,9,1,3};
        int n = arr.length;
        int largest = arr[0];
        for(int i=1;i<n;i++){
            if(arr[i]>largest)
                largest = arr[i]; // update the largest value;
        }
        System.out.println("largest element is "+largest);
    }
}
