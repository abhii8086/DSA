public class patterns {

	public static void main(String[] args) {
//        * * * * *
//        * * * *
//        * * * 
//        * * 
//        * 
		int rows = 5;
		int col = 5;
		int k = 6;

		for (int i = 1; i <= rows; i++) {

			for (int j = 1; j <= k - i; j++) {

				System.out.print(" " + "*");
			}
			System.out.println();
		}

	}

}
